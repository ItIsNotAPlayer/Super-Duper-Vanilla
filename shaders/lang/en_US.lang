    screen.NETHER_SETTINGS = Nether settings
    screen.NETHER_SETTINGS.comment = §cNether fog and sky color configuration.
        option.FOGn1_VERTICAL_DENSITY = Vertical density
        option.FOGn1_VERTICAL_DENSITY.comment = Vertical fog density for the Nether. Higher values = thinner fog at high altitudes, thicker at low.

        option.FOGn1_TOTAL_DENSITY = Total density
        option.FOGn1_TOTAL_DENSITY.comment = Total fog density for the Nether. Higher values = thicker fog.

        option.WORLDn1_VANILLA_FOGCOLI = Sky intensity
        option.WORLDn1_VANILLA_FOGCOLI.comment = Intensity for Nether vanilla fog color.

    screen.END_SETTINGS = End settings
    screen.END_SETTINGS.comment = §5End dimension light, sky, and fog color configuration.
        option.LIGHT1_CR = Light red
        option.LIGHT1_CR.comment = Red channel for End light color.

        option.LIGHT1_CG = Light green
        option.LIGHT1_CG.comment = Green channel for End light color.

