            option.SKY0_NB = Sky blue
            option.SKY0_NB.comment = Blue channel for night sky color.
            
            option.SKY0_NI = Sky intensity
            option.SKY0_NI.comment = Intensity for night sky color.

        screen.TWILIGHT_SETTINGS = Twilight settings
        screen.TWILIGHT_SETTINGS.comment = §dTwilight light and sky color configuration.
            option.LIGHT0_TR = Light red
            option.LIGHT0_TR.comment = Red channel for twilight light color.
            
            option.LIGHT0_TG = Light green
            option.LIGHT0_TG.comment = Green channel for twilight light color.
            
            option.LIGHT0_TB = Light blue
            option.LIGHT0_TB.comment = Blue channel for twilight light color.
            
            option.LIGHT0_TI = Light intensity
            option.LIGHT0_TI.comment = Intensity for twilight light color.

            option.SKY0_TR = Sky red
            option.SKY0_TR.comment = Red channel for twilight sky color.
            
            option.SKY0_TG = Sky green
            option.SKY0_TG.comment = Green channel for twilight sky color.
            
            option.SKY0_TB = Sky blue
            option.SKY0_TB.comment = Blue channel for twilight sky color.
            
            option.SKY0_TI = Sky intensity
            option.SKY0_TI.comment = Intensity for twilight sky color.

        screen.FOG_SETTINGS0 = Fog settings
        screen.FOG_SETTINGS0.comment = §7Fog density and color settings for the overworld.
            option.FOG0_VERTICAL_DENSITY_D = Vertical density day
            option.FOG0_VERTICAL_DENSITY_D.comment = Vertical fog density for day. Higher values = thinner fog at high altitudes, thicker at low.
            
            option.FOG0_VERTICAL_DENSITY_N = Vertical density night
            option.FOG0_VERTICAL_DENSITY_N.comment = Vertical fog density for night.
            
            option.FOG0_VERTICAL_DENSITY_T = Vertical density twilight
            option.FOG0_VERTICAL_DENSITY_T.comment = Vertical fog density for twilight.
            
            option.FOG0_TOTAL_DENSITY = Total density
